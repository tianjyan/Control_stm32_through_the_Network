<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <!--UI-->
    <system:String x:Key="UI_DeviceID">Name:</system:String>
    <system:String x:Key="UI_DeviceIP">IP:</system:String>
    <system:String x:Key="UI_DevicePort">Port:</system:String>
    <system:String x:Key="UI_DevicePassword">Password:</system:String>

    <!--MSG-->
    <system:String x:Key="MSG_Error">Error</system:String>
</ResourceDictionary>