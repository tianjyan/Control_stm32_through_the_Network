<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <!--UI-->
    <system:String x:Key="UI_DeviceID">硬件名称:</system:String>
    <system:String x:Key="UI_DeviceIP">硬件地址:</system:String>
    <system:String x:Key="UI_DevicePort">硬件端口:</system:String>
    <system:String x:Key="UI_DevicePassword">硬件密码:</system:String>

    <!--MSG-->
    <system:String x:Key="MSG_Error">Error</system:String>

</ResourceDictionary>