    <system:String x:Key="UI_Analog">模拟输入</system:String>
    <system:String x:Key="UI_Impulse">脉冲宽度</system:String>
    <system:String x:Key="UI_Humiture">温度湿度</system:String>
    <system:String x:Key="UI_IR">红外收发</system:String>

    <system:String x:Key="UI_GetDigit">获取数字状态</system:String>
    <system:String x:Key="UI_GetAnalog">获取模拟状态</system:String>



    <!--MSG-->
    <system:String x:Key="MSG_Error">Error</system:String>

</ResourceDictionary>